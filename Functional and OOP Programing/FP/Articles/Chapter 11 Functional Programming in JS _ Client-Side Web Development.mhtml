    <span class=3D"token function">funcToCall</span><span class=3D"token pu=
nctuation">(</span><span class=3D"token string">"world"</span><span class=
=3D"token punctuation">)</span><span class=3D"token punctuation">;</span>
<span class=3D"token punctuation">}</span>

<span class=3D"token comment">//pass the named function by name</span>
<span class=3D"token function">doWithWorld</span><span class=3D"token punct=
uation">(</span>sayHello<span class=3D"token punctuation">)</span><span cla=
ss=3D"token punctuation">;</span>

<span class=3D"token comment">//pass in anonymous version of the function</=
span>
<span class=3D"token function">doWithWorld</span><span class=3D"token punct=
